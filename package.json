    "start:prod": "npm run clean:server && npm run build:server && node build/start/heyneighbor-server.prod",
    "packager": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "flow": "flow",
    "lint": "eslint .",
    "lint:changed": "git diff HEAD --name-only --diff-filter=ACM | grep '.js$' | xargs eslint",
    "build:android": "./android/gradlew -p android assembleRelease --stacktrace",
    "build:server": "babel --source-maps --ignore **/__tests__/** --out-dir build/ src/",
    "build:client": "webpack",
    "build": "npm run clean:client && npm run build:client",
    "clean:server": "rm -rf build",
    "clean:client": "rm -rf static/dist",
    "clean": "npm run clean:server && npm run clean:client",
    "postinstall": "for file in `ls .git-hooks`; do ln -sf ../../.git-hooks/${file} .git/hooks/${file}; done"
